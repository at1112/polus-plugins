                "RichardsonLucyUpdate"
            ],
            "description": "Operation to perform",
            "required": true
        },
        "in1": {
            "type": "collection",
            "title": "in1",
            "description": "in1",
            "required": true,
            "call_types": {
                "RichardsonLucyCorrection": "RandomAccessibleInterval"
            }
        },
        "in2": {
            "type": "collection",
            "title": "in2",
            "description": "in2",
            "required": true,
            "call_types": {
                "RichardsonLucyCorrection": "RandomAccessibleInterval"
            }
        },
        "fftBuffer": {
            "type": "collection",
            "title": "fftBuffer",
            "description": "fftBuffer",
            "required": true,
            "call_types": {
                "RichardsonLucyCorrection": "RandomAccessibleInterval"
            }
        },
        "fftKernel": {
            "type": "collection",
            "title": "fftKernel",
            "description": "fftKernel",
            "required": true,
            "call_types": {
                "RichardsonLucyCorrection": "RandomAccessibleInterval"
            }
        },
        "in": {
            "type": "collection",
            "title": "in",
            "description": "in",
            "required": true,
            "call_types": {
                "RichardsonLucyUpdate": "RandomAccessibleInterval"
            }
        }
    },
    "_outputs": {
        "out": {
            "type": "collection",
            "title": "out",
            "description": "out",
            "call_types": {
                "RichardsonLucyCorrection": "RandomAccessibleInterval",
                "RichardsonLucyUpdate": "RandomAccessibleInterval"
            }
        }
    },
    "project_slug": "polus-{{ cookiecutter.project_name|lower|replace(' ', '-') }}-plugin"
