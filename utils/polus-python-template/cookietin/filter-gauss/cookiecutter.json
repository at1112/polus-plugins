                "GaussRAISingleSigma": "double"
            }
        },
        "sigmas": {
            "type": "array",
            "title": "sigmas",
            "description": "sigmas",
            "required": true,
            "call_types": {
                "DefaultGaussRAI": "double[]"
            }
        }
    },
    "_outputs": {
        "out": {
            "type": "collection",
            "title": "out",
